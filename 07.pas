Program Pzim ;
	var
	a:real;
	b:real;
	soma,subtracao,divisao,multiplicacao:real;
	
	
Begin
	writeln ('Digite seu primeiro numero');
	read (a);
	writeln ('Digite seu segundo numero');
	read (b);
	soma:=a+b;
	subtracao:=a-b;
	divisao:=a/b;
	multiplicacao:=a*b;
	writeln(soma);
	writeln(subtracao);
	writeln (divisao);
	writeln (multiplicacao);
	
	
End.